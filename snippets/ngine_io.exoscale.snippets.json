{
    "Manages domain records on Exoscale DNS API.": {
        "prefix": [
            "ngine_io.exoscale.exo_dns_domain",
            "Manages domain records on Exoscale DNS API."
        ],
        "body": [
            "- name: ${1:Description}",
            "  # ${99:Comment}",
            "  ngine_io.exoscale.exo_dns_domain:",
            "    api_key: ${2:string}",
            "    api_region: ${3:cloudstack}",
            "    api_secret: ${4:string}",
            "    api_timeout: ${5:10}",
            "    name: ${6:string}",
            "    state: ${7|present,absent|}",
            "    validate_certs: ${8|true,false|}",
            "$0"
        ],
        "description": "Manages domain records on Exoscale DNS API.",
        "scope": "ansible"
    },
    "Manages DNS records on Exoscale DNS.": {
        "prefix": [
            "ngine_io.exoscale.exo_dns_record",
            "Manages DNS records on Exoscale DNS."
        ],
        "body": [
            "- name: ${1:Description}",
            "  # ${99:Comment}",
            "  ngine_io.exoscale.exo_dns_record:",
            "    api_key: ${2:string}",
            "    api_region: ${3:cloudstack}",
            "    api_secret: ${4:string}",
            "    api_timeout: ${5:10}",
            "    content: ${6:string}",
            "    domain: ${7:string}",
            "    multiple: ${8|false,true|}",
            "    name: ${9:}",
            "    prio: ${10:int}",
            "    record_type: ${11|A,ALIAS,CNAME,MX,SPF,URL,TXT,NS,SRV,NAPTR,PTR,AAAA,SSHFP,HINFO,POOL|}",
            "    state: ${12|present,absent|}",
            "    ttl: ${13:3600}",
            "    validate_certs: ${14|true,false|}",
            "$0"
        ],
        "description": "Manages DNS records on Exoscale DNS.",
        "scope": "ansible"
    }
}