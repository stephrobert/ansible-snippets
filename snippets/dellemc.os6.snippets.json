{
    "Run commands on devices running Dell EMC OS6": {
        "prefix": [
            "dellemc.os6.os6_command",
            "Run commands on devices running Dell EMC OS6"
        ],
        "body": [
            "- name: ${1:Description}",
            "  # ${99:Comment}",
            "  dellemc.os6.os6_command:",
            "    commands:",
            "      - ${2:element}",
            "    interval: ${3:1}",
            "    match: ${4|all,any|}",
            "    retries: ${6:10}",
            "    wait_for:",
            "      - ${7:element}",
            "$0"
        ],
        "description": "Run commands on devices running Dell EMC OS6",
        "scope": "ansible"
    },
    "Manage Dell EMC OS6 configuration sections": {
        "prefix": [
            "dellemc.os6.os6_config",
            "Manage Dell EMC OS6 configuration sections"
        ],
        "body": [
            "- name: ${1:Description}",
            "  # ${99:Comment}",
            "  dellemc.os6.os6_config:",
            "    after:",
            "      - ${2:element}",
            "    backup: ${3|true,false|}",
            "    before:",
            "      - ${5:element}",
            "    config: ${6:string}",
            "    lines:",
            "      - ${7:element}",
            "    match: ${8|line,strict,exact,none|}",
            "    parents:",
            "      - ${9:element}",
            "    replace: ${11|line,block|}",
            "    save: ${12|true,false|}",
            "    src: ${13:path}",
            "    update: ${14|merge,check|}",
            "$0"
        ],
        "description": "Manage Dell EMC OS6 configuration sections",
        "scope": "ansible"
    },
    "Collect facts from devices running Dell EMC OS6": {
        "prefix": [
            "dellemc.os6.os6_facts",
            "Collect facts from devices running Dell EMC OS6"
        ],
        "body": [
            "- name: ${1:Description}",
            "  # ${99:Comment}",
            "  dellemc.os6.os6_facts:",
            "    gather_subset:",
            "      - ${2:element}",
            "$0"
        ],
        "description": "Collect facts from devices running Dell EMC OS6",
        "scope": "ansible"
    }
}