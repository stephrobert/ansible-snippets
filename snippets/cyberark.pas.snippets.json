{
    "Module for CyberArk Account object creation, deletion, and modification using PAS Web Services SDK.": {
        "prefix": [
            "cyberark.pas.cyberark_account",
            "Module for CyberArk Account object creation, deletion, and modification using PAS Web Services SDK."
        ],
        "body": [
            "- name: ${1:Description}",
            "  # ${99:Comment}",
            "  cyberark.pas.cyberark_account:",
            "    address: ${2:string}",
            "    api_base_url: ${3:string}",
            "    identified_by: ${5:username,address,platform_id}",
            "    logging_file: ${6:/tmp/ansible_cyberark.log}",
            "    logging_level: ${7:string}",
            "    name: ${8:string}",
            "    new_secret: ${9:string}",
            "    platform_id: ${11:string}",
            "    safe: ${13:string}",
            "    secret: ${14:string}",
            "    secret_type: ${16|password,key|}",
            "    state: ${17|present,absent|}",
            "    username: ${18:string}",
            "    validate_certs: ${19|true,false|}",
            "$0"
        ],
        "description": "Module for CyberArk Account object creation, deletion, and modification using PAS Web Services SDK.",
        "scope": "ansible"
    },
    "CyberArk Authentication using PAS Web Services SDK.": {
        "prefix": [
            "cyberark.pas.cyberark_authentication",
            "CyberArk Authentication using PAS Web Services SDK."
        ],
        "body": [
            "- name: ${1:Description}",
            "  # ${99:Comment}",
            "  cyberark.pas.cyberark_authentication:",
            "    api_base_url: ${2:string}",
            "    concurrentSession: ${3|false,true|}",
            "    connection_number: ${4:int}",
            "    new_password: ${6:string}",
            "    password: ${7:string}",
            "    state: ${8|present,absent|}",
            "    timeout: ${9:10}",
            "    use_cyberark_authentication: ${10|true,false|}",
            "    use_ldap_authentication: ${11|true,false|}",
            "    use_radius_authentication: ${12|true,false|}",
            "    use_windows_authentication: ${13|true,false|}",
            "    username: ${14:string}",
            "    validate_certs: ${15|true,false|}",
            "$0"
        ],
        "description": "CyberArk Authentication using PAS Web Services SDK.",
        "scope": "ansible"
    },
    "Credential retrieval using AAM Central Credential Provider.": {
        "prefix": [
            "cyberark.pas.cyberark_credential",
            "Credential retrieval using AAM Central Credential Provider."
        ],
        "body": [
            "- name: ${1:Description}",
            "  # ${99:Comment}",
            "  cyberark.pas.cyberark_credential:",
            "    api_base_url: ${2:string}",
            "    app_id: ${3:string}",
            "    client_cert: ${4:string}",
            "    client_key: ${5:string}",
            "    connection_timeout: ${6:30}",
            "    fail_request_on_password_change: ${7|false,true|}",
            "    query: ${8:string}",
            "    query_format: ${9|Exact,Regexp|}",
            "    reason: ${10:string}",
            "    validate_certs: ${11|true,false|}",
            "$0"
        ],
        "description": "Credential retrieval using AAM Central Credential Provider.",
        "scope": "ansible"
    },
    "CyberArk User Management using PAS Web Services SDK.": {
        "prefix": [
            "cyberark.pas.cyberark_user",
            "CyberArk User Management using PAS Web Services SDK."
        ],
        "body": [
            "- name: ${1:Description}",
            "  # ${99:Comment}",
            "  cyberark.pas.cyberark_user:",
            "    authorization:",
            "      - ${2:element}",
            "    change_password_on_the_next_logon: ${3|false,true|}",
            "    disabled: ${5|false,true|}",
            "    domain_name: ${6:string}",
            "    email: ${7:string}",
            "    expiry_date: ${8:string}",
            "    first_name: ${9:string}",
            "    group_name: ${10:string}",
            "    initial_password: ${11:string}",
            "    last_name: ${12:string}",
            "    location: ${13:string}",
            "    logging_file: ${14:/tmp/ansible_cyberark.log}",
            "    logging_level: ${15|NOTSET,DEBUG,INFO|}",
            "    member_type: ${16:string}",
            "    new_password: ${17:string}",
            "    state: ${18|present,absent|}",
            "    user_type_name: ${20:string}",
            "    username: ${21:string}",
            "    vault_id: ${22:int}",
            "$0"
        ],
        "description": "CyberArk User Management using PAS Web Services SDK.",
        "scope": "ansible"
    }
}