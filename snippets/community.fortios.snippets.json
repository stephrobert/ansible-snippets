        "prefix": [
            "community.fortios.faz_device",
            "Add or remove device"
        ],
        "body": [
            "- name: ${1:Description}",
            "  # ${99:Comment}",
            "  community.fortios.faz_device:",
            "    adom: ${2:root}",
            "    device_ip: ${3:string}",
            "    device_password: ${4:string}",
            "    device_serial: ${5:string}",
            "    device_unique_name: ${6:string}",
            "    device_username: ${7:string}",
            "    faz_quota: ${8:string}",
