{
    "Access-Lists resource module": {
        "prefix": [
            "cisco.asa.asa_acls",
            "Access-Lists resource module"
        ],
        "body": [
            "- name: ${1:Description}",
            "  # ${99:Comment}",
            "  cisco.asa.asa_acls:",
            "    running_config: ${3:string}",
            "    state: ${4|merged,replaced,overridden,deleted,gathered,rendered,parsed|}",
            "$0"
        ],
        "description": "Access-Lists resource module",
        "scope": "ansible"
    },
    "Run arbitrary commands on Cisco ASA devices": {
        "prefix": [
            "cisco.asa.asa_command",
            "Run arbitrary commands on Cisco ASA devices"
        ],
        "body": [
            "- name: ${1:Description}",
            "  # ${99:Comment}",
            "  cisco.asa.asa_command:",
            "    commands:",
            "      - ${2:element}",
            "    context: ${3:string}",
            "    interval: ${4:1}",
            "    match: ${5|all,any|}",
            "    passwords: ${6|false,true|}",
            "    retries: ${7:10}",
            "    wait_for:",
            "      - ${8:element}",
            "$0"
        ],
        "description": "Run arbitrary commands on Cisco ASA devices",
        "scope": "ansible"
    },
    "Manage configuration sections on Cisco ASA devices": {
        "prefix": [
            "cisco.asa.asa_config",
            "Manage configuration sections on Cisco ASA devices"
        ],
        "body": [
            "- name: ${1:Description}",
            "  # ${99:Comment}",
            "  cisco.asa.asa_config:",
            "    after:",
            "      - ${2:element}",
            "    backup: ${3|false,true|}",
            "    before:",
            "      - ${5:element}",
            "    config: ${6:string}",
            "    context: ${7:string}",
            "    defaults: ${8|false,true|}",
            "    lines:",
            "      - ${9:element}",
            "    match: ${10|line,strict,exact,none|}",
            "    parents:",
            "      - ${11:element}",
            "    passwords: ${12|false,true|}",
            "    replace: ${13|line,block|}",
            "    save: ${14|false,true|}",
            "    save_when: ${15|never,always,modified,changed|}",
            "    src: ${16:path}",
            "$0"
        ],
        "description": "Manage configuration sections on Cisco ASA devices",
        "scope": "ansible"
    },
    "Collect facts from remote devices running Cisco ASA": {
        "prefix": [
            "cisco.asa.asa_facts",
            "Collect facts from remote devices running Cisco ASA"
        ],
        "body": [
            "- name: ${1:Description}",
            "  # ${99:Comment}",
            "  cisco.asa.asa_facts:",
            "    context: ${2:string}",
            "    gather_network_resources:",
            "      - ${3:element}",
            "    gather_subset:",
            "      - ${4:element}",
            "    passwords: ${5|false,true|}",
            "$0"
        ],
        "description": "Collect facts from remote devices running Cisco ASA",
        "scope": "ansible"
    },
    "Object Group resource module": {
        "prefix": [
            "cisco.asa.asa_ogs",
            "Object Group resource module"
        ],
        "body": [
            "- name: ${1:Description}",
            "  # ${99:Comment}",
            "  cisco.asa.asa_ogs:",
            "    config:",
            "      - ${2:element}",
            "    running_config: ${3:string}",
            "    state: ${4|merged,replaced,overridden,deleted,gathered,rendered,parsed|}",
            "$0"
        ],
        "description": "Object Group resource module",
        "scope": "ansible"
    }
}