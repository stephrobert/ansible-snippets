{
    "Manage custom images on the cloudscale.ch IaaS service": {
        "prefix": [
            "cloudscale_ch.cloud.custom_image",
            "Manage custom images on the cloudscale.ch IaaS service"
        ],
        "body": [
            "- name: ${1:Description}",
            "  # ${99:Comment}",
            "  cloudscale_ch.cloud.custom_image:",
            "    api_timeout: ${2:45}",
            "    api_token: ${3:string}",
            "    api_url: ${4:https://api.cloudscale.ch/v1}",
            "    firmware_type: ${5|bios,uefi|}",
            "    force_retry: ${6|false,true|}",
            "    name: ${7:string}",
            "    slug: ${8:string}",
            "    source_format: ${9:string}",
            "    state: ${10|present,absent|}",
            "    url: ${12:string}",
            "    user_data_handling: ${13:string}",
            "    uuid: ${14:string}",
            "    zones:",
            "      - ${15:element}",
            "$0"
        ],
        "description": "Manage custom images on the cloudscale.ch IaaS service",
        "scope": "ansible"
    },
