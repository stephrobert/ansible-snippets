{
    "Manage Alerting Contact points in Grafana Cloud": {
        "prefix": [
            "grafana.grafana.alert_contact_point",
            "Manage Alerting Contact points in Grafana Cloud"
        ],
        "body": [
            "- name: ${1:Description}",
            "  # ${99:Comment}",
            "  grafana.grafana.alert_contact_point:",
            "    disableResolveMessage: ${2|false,true|}",
            "    grafana_api_key: ${3:string}",
            "    name: ${4:string}",
            "    stack_slug: ${6:string}",
            "    state: ${7|present,absent|}",
            "    type: ${8:string}",
            "    uid: ${9:string}",
            "$0"
        ],
        "description": "Manage Alerting Contact points in Grafana Cloud",
        "scope": "ansible"
    },
    "Sets the notification policy tree in Alerting on Grafana Cloud": {
        "prefix": [
            "grafana.grafana.alert_notification_policy",
            "Sets the notification policy tree in Alerting on Grafana Cloud"
        ],
        "body": [
            "- name: ${1:Description}",
            "  # ${99:Comment}",
            "  grafana.grafana.alert_notification_policy:",
            "    Continue: ${2|false,true|}",
            "    grafana_api_key: ${3:string}",
            "    groupByStr:",
            "      - ${4:element}",
            "    groupInterval: ${5:5m}",
            "    groupWait: ${6:30s}",
            "    muteTimeIntervals:",
            "      - ${7:element}",
            "    objectMatchers:",
            "      - ${8:element}",
            "    repeatInterval: ${9:4h}",
            "    rootPolicyReceiver: ${10:grafana-default-email}",
            "    routes:",
            "      - ${11:element}",
            "    stack_slug: ${12:string}",
            "$0"
        ],
        "description": "Sets the notification policy tree in Alerting on Grafana Cloud",
        "scope": "ansible"
    },
    "Manage Grafana Cloud API keys": {
        "prefix": [
            "grafana.grafana.cloud_api_key",
            "Manage Grafana Cloud API keys"
        ],
        "body": [
            "- name: ${1:Description}",
            "  # ${99:Comment}",
            "  grafana.grafana.cloud_api_key:",
            "    existing_cloud_api_key: ${2:string}",
            "    fail_if_already_created: ${3|true,false|}",
            "    name: ${4:string}",
            "    org_slug: ${5:string}",
            "    role: ${6:string}",
            "    state: ${7|present,absent|}",
            "$0"
        ],
        "description": "Manage Grafana Cloud API keys",
        "scope": "ansible"
    },
    "Manage Grafana Cloud Plugins": {
        "prefix": [
            "grafana.grafana.cloud_plugin",
            "Manage Grafana Cloud Plugins"
        ],
        "body": [
            "- name: ${1:Description}",
            "  # ${99:Comment}",
            "  grafana.grafana.cloud_plugin:",
            "    cloud_api_key: ${2:string}",
            "    name: ${3:string}",
            "    stack_slug: ${4:string}",
            "    state: ${5|present,absent|}",
            "    version: ${6:latest}",
            "$0"
        ],
        "description": "Manage Grafana Cloud Plugins",
        "scope": "ansible"
    },
    "Manage Grafana Cloud stack": {
        "prefix": [
            "grafana.grafana.cloud_stack",
            "Manage Grafana Cloud stack"
        ],
        "body": [
            "- name: ${1:Description}",
            "  # ${99:Comment}",
            "  grafana.grafana.cloud_stack:",
            "    cloud_api_key: ${2:string}",
            "    name: ${3:string}",
            "    org_slug: ${4:string}",
            "    region: ${5|us,us-azure,eu,au,eu-azure,prod-ap-southeast-0,prod-gb-south-0,prod-eu-west-3|}",
            "    stack_slug: ${6:string}",
            "    state: ${7|present,absent|}",
            "    url: ${8:string}",
            "$0"
        ],
        "description": "Manage Grafana Cloud stack",
        "scope": "ansible"
    },
    "Manage Dashboards in Grafana Cloud": {
        "prefix": [
            "grafana.grafana.dashboard",
            "Manage Dashboards in Grafana Cloud"
        ],
        "body": [
            "- name: ${1:Description}",
            "  # ${99:Comment}",
            "  grafana.grafana.dashboard:",
            "    grafana_api_key: ${3:string}",
            "    stack_slug: ${4:string}",
            "    state: ${5|present,absent|}",
            "$0"
        ],
        "description": "Manage Dashboards in Grafana Cloud",
        "scope": "ansible"
    },
    "Manage Data sources in Grafana Cloud": {
        "prefix": [
            "grafana.grafana.datasource",
            "Manage Data sources in Grafana Cloud"
        ],
        "body": [
            "- name: ${1:Description}",
            "  # ${99:Comment}",
            "  grafana.grafana.datasource:",
            "    grafana_api_key: ${3:string}",
            "    stack_slug: ${4:string}",
            "    state: ${5|present,absent|}",
            "$0"
        ],
        "description": "Manage Data sources in Grafana Cloud",
        "scope": "ansible"
    },
    "Manage Folders in Grafana Cloud": {
        "prefix": [
            "grafana.grafana.folder",
            "Manage Folders in Grafana Cloud"
        ],
        "body": [
            "- name: ${1:Description}",
            "  # ${99:Comment}",
            "  grafana.grafana.folder:",
            "    grafana_api_key: ${2:string}",
            "    overwrite: ${3|true,false|}",
            "    stack_slug: ${4:string}",
            "    state: ${5|present,absent|}",
            "    title: ${6:string}",
            "    uid: ${7:string}",
            "$0"
        ],
        "description": "Manage Folders in Grafana Cloud",
        "scope": "ansible"
    }
}