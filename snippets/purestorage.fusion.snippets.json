    "Manage API clients in Pure Storage Fusion": {
        "prefix": [
            "purestorage.fusion.fusion_api_client",
            "Manage API clients in Pure Storage Fusion"
        ],
        "body": [
            "- name: ${1:Description}",
            "  # ${99:Comment}",
            "  purestorage.fusion.fusion_api_client:",
            "    app_id: ${2:string}",
            "    key_file: ${3:string}",
            "    name: ${4:string}",
            "    public_key: ${5:string}",
            "    state: ${6|present,absent|}",
            "$0"
        ],
        "description": "Manage API clients in Pure Storage Fusion",
        "scope": "ansible"
    },
