    "This module will manage a user entities in a SAP S4/HANA environment": {
        "prefix": [
            "community.sap_libs.sap_user",
            "This module will manage a user entities in a SAP S4/HANA environment"
        ],
        "body": [
            "- name: ${1:Description}",
            "  # ${99:Comment}",
            "  community.sap_libs.sap_user:",
            "    client: ${2:000}",
            "    company: ${3:string}",
            "    conn_password: ${4:string}",
            "    conn_username: ${5:string}",
            "    email: ${6:string}",
            "    firstname: ${7:string}",
            "    force: ${8|false,true|}",
            "    host: ${9:string}",
            "    lastname: ${10:string}",
            "    password: ${11:string}",
            "    profiles:",
            "      - ${12:element}",
            "    roles:",
            "      - ${13:element}",
            "    state: ${14|present,absent,lock,unlock|}",
            "    sysnr: ${15:00}",
            "    user_type: ${16|A,B,C,S,L|}",
            "    useralias: ${17:string}",
            "    username: ${18:string}",
