{
    "This module will manage a company entities in a SAP S4HANA environment": {
        "prefix": [
            "community.sap_libs.sap_company",
            "This module will manage a company entities in a SAP S4HANA environment"
        ],
        "body": [
            "- name: ${1:Description}",
            "  # ${99:Comment}",
            "  community.sap_libs.sap_company:",
            "    city: ${2:string}",
            "    client: ${3:000}",
            "    company_id: ${4:string}",
            "    conn_password: ${5:string}",
            "    conn_username: ${6:string}",
            "    country: ${7:string}",
            "    e_mail: ${8:string}",
            "    host: ${9:string}",
            "    name: ${10:string}",
            "    name_2: ${11:string}",
            "    post_code: ${12:string}",
            "    state: ${13|present,absent|}",
            "    street: ${14:string}",
            "    street_no: ${15:string}",
            "    sysnr: ${16:01}",
            "    time_zone: ${17:string}",
            "$0"
        ],
        "description": "This module will manage a company entities in a SAP S4HANA environment",
        "scope": "ansible"
    },
    "Ansible Module to execute SAPCONTROL": {
        "prefix": [
            "community.sap_libs.sap_control_exec",
            "Ansible Module to execute SAPCONTROL"
        ],
        "body": [
            "- name: ${1:Description}",
            "  # ${99:Comment}",
            "  community.sap_libs.sap_control_exec:",
            "    force: ${2|false,true|}",
            "    function: ${3:string}",
            "    hostname: ${4:localhost}",
            "    parameter: ${5:string}",
            "    password: ${6:string}",
            "    port: ${7:int}",
            "    sysnr: ${8:string}",
            "    username: ${9:string}",
            "$0"
        ],
        "description": "Ansible Module to execute SAPCONTROL",
        "scope": "ansible"
    },
    "Ansible Module to execute SQL on SAP HANA": {
        "prefix": [
            "community.sap_libs.sap_hdbsql",
            "Ansible Module to execute SQL on SAP HANA"
        ],
        "body": [
            "- name: ${1:Description}",
            "  # ${99:Comment}",
            "  community.sap_libs.sap_hdbsql:",
            "    autocommit: ${2|true,false|}",
            "    bin_path: ${3:string}",
            "    database: ${4:string}",
            "    encrypted: ${5|false,true|}",
            "    filepath:",
            "      - ${6:element}",
            "    host: ${7:string}",
            "    instance: ${8:string}",
            "    password: ${9:string}",
            "    query:",
            "      - ${10:element}",
            "    sid: ${11:string}",
            "    user: ${12:SYSTEM}",
            "    userstore: ${13|false,true|}",
            "$0"
        ],
        "description": "Ansible Module to execute SQL on SAP HANA",
        "scope": "ansible"
    },
    "Ansible Module for use of SAP PyRFC to execute SAP RFCs (Remote Function Calls) to SAP remote-enabled function modules": {
