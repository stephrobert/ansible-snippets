    "Manages SAP SAPCAR archives": {
        "prefix": [
            "community.sap_libs.sapcar_extract",
            "Manages SAP SAPCAR archives"
        ],
        "body": [
            "- name: ${1:Description}",
            "  # ${99:Comment}",
            "  community.sap_libs.sapcar_extract:",
            "    binary_path: ${2:path}",
            "    dest: ${3:path}",
            "    manifest: ${4:SIGNATURE.SMF}",
            "    path: ${5:path}",
            "    remove: ${6|false,true|}",
            "    security_library: ${7:path}",
            "    signature: ${8|false,true|}",
            "$0"
        ],
        "description": "Manages SAP SAPCAR archives",
        "scope": "ansible"
    }
