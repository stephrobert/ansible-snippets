    "Manage Arista EOS configuration sections": {
        "prefix": [
            "arista.eos.eos_config",
            "Manage Arista EOS configuration sections"
        ],
        "body": [
            "- name: ${1:Description}",
            "  # ${99:Comment}",
            "  arista.eos.eos_config:",
            "    after:",
            "      - ${2:element}",
            "    backup: ${3|false,true|}",
            "    before:",
            "      - ${5:element}",
            "    defaults: ${6|false,true|}",
            "    diff_against: ${7|session,startup,running,intended,validate_config|}",
            "    diff_ignore_lines:",
            "      - ${8:element}",
            "    intended_config: ${9:string}",
            "    lines:",
            "      - ${10:element}",
            "    match: ${11|line,strict,exact,none|}",
            "    parents:",
            "      - ${12:element}",
            "    replace: ${13|line,block,config|}",
            "    running_config: ${14:string}",
