{
    "Run commands on remote devices running Dell OS9": {
        "prefix": [
            "dellemc.os9.os9_command",
            "Run commands on remote devices running Dell OS9"
        ],
        "body": [
            "- name: ${1:Description}",
            "  # ${99:Comment}",
            "  dellemc.os9.os9_command:",
            "    commands:",
            "      - ${2:element}",
            "    interval: ${3:1}",
            "    match: ${4|all,any|}",
            "    retries: ${6:10}",
            "    wait_for:",
            "      - ${7:element}",
            "$0"
        ],
        "description": "Run commands on remote devices running Dell OS9",
        "scope": "ansible"
    },
    "Manage Dell EMC Networking OS9 configuration sections": {
        "prefix": [
            "dellemc.os9.os9_config",
            "Manage Dell EMC Networking OS9 configuration sections"
        ],
        "body": [
            "- name: ${1:Description}",
            "  # ${99:Comment}",
            "  dellemc.os9.os9_config:",
            "    after:",
            "      - ${2:element}",
            "    backup: ${3|true,false|}",
            "    before:",
            "      - ${5:element}",
            "    config: ${6:string}",
            "    lines:",
            "      - ${7:element}",
            "    match: ${8|line,strict,exact,none|}",
            "    parents:",
            "      - ${9:element}",
            "    replace: ${11|line,block|}",
            "    save: ${12|false,true|}",
            "    src: ${13:path}",
            "    update: ${14|merge,check|}",
            "$0"
        ],
        "description": "Manage Dell EMC Networking OS9 configuration sections",
        "scope": "ansible"
    },
    "Collect facts from remote devices running Dell EMC Networking OS9": {
        "prefix": [
            "dellemc.os9.os9_facts",
            "Collect facts from remote devices running Dell EMC Networking OS9"
        ],
        "body": [
            "- name: ${1:Description}",
            "  # ${99:Comment}",
            "  dellemc.os9.os9_facts:",
            "    gather_subset:",
            "      - ${2:element}",
            "$0"
        ],
        "description": "Collect facts from remote devices running Dell EMC Networking OS9",
        "scope": "ansible"
    }
}