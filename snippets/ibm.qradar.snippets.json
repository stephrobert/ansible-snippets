{
    "Trigger a qradar configuration deployment": {
        "prefix": [
            "ibm.qradar.deploy",
            "Trigger a qradar configuration deployment"
        ],
        "body": [
            "- name: ${1:Description}",
            "  # ${99:Comment}",
            "  ibm.qradar.deploy:",
            "    type: ${2|INCREMENTAL,FULL|}",
            "$0"
        ],
        "description": "Trigger a qradar configuration deployment",
        "scope": "ansible"
    },
    "Take action on a QRadar Offense": {
        "prefix": [
            "ibm.qradar.offense_action",
            "Take action on a QRadar Offense"
        ],
        "body": [
            "- name: ${1:Description}",
            "  # ${99:Comment}",
            "  ibm.qradar.offense_action:",
            "    assigned_to: ${2:string}",
            "    closing_reason: ${3:string}",
            "    closing_reason_id: ${4:int}",
            "    follow_up: ${5|false,true|}",
            "    id: ${6:int}",
            "    protected: ${7|false,true|}",
            "    status: ${8:string}",
            "$0"
        ],
        "description": "Take action on a QRadar Offense",
        "scope": "ansible"
    },
    "Obtain information about one or many QRadar Offenses, with filter options": {
        "prefix": [
            "ibm.qradar.offense_info",
            "Obtain information about one or many QRadar Offenses, with filter options"
        ],
        "body": [
            "- name: ${1:Description}",
            "  # ${99:Comment}",
            "  ibm.qradar.offense_info:",
            "    assigned_to: ${2:string}",
            "    closing_reason: ${3:string}",
            "    closing_reason_id: ${4:int}",
            "    follow_up: ${5|false,true|}",
            "    id: ${6:int}",
            "    name: ${7:string}",
            "    protected: ${8|false,true|}",
            "    status: ${9|open,OPEN,hidden,HIDDEN,closed,CLOSED|}",
            "$0"
        ],
        "description": "Obtain information about one or many QRadar Offenses, with filter options",
        "scope": "ansible"
    },
    "Create or update a QRadar Offense Note": {
        "prefix": [
            "ibm.qradar.offense_note",
            "Create or update a QRadar Offense Note"
        ],
        "body": [
            "- name: ${1:Description}",
            "  # ${99:Comment}",
            "  ibm.qradar.offense_note:",
            "    id: ${2:int}",
            "    note_text: ${3:string}",
            "$0"
        ],
        "description": "Create or update a QRadar Offense Note",
        "scope": "ansible"
    },
    "Qradar Analytics Rules Management resource module": {
        "prefix": [
            "ibm.qradar.qradar_analytics_rules",
            "Qradar Analytics Rules Management resource module"
        ],
        "body": [
            "- name: ${1:Description}",
            "  # ${99:Comment}",
            "  ibm.qradar.qradar_analytics_rules:",
            "    state: ${3:string}",
            "$0"
        ],
        "description": "Qradar Analytics Rules Management resource module",
        "scope": "ansible"
    },
    "Qradar Log Sources Management resource module": {
        "prefix": [
            "ibm.qradar.qradar_log_sources_management",
            "Qradar Log Sources Management resource module"
        ],
        "body": [
            "- name: ${1:Description}",
            "  # ${99:Comment}",
            "  ibm.qradar.qradar_log_sources_management:",
            "    config:",
            "      - ${2:element}",
            "    state: ${3:string}",
            "$0"
        ],
        "description": "Qradar Log Sources Management resource module",
        "scope": "ansible"
    }
}