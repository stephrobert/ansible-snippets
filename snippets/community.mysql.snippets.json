    "Add or remove MySQL databases from a remote host": {
        "prefix": [
            "community.mysql.mysql_db",
            "Add or remove MySQL databases from a remote host"
        ],
        "body": [
            "- name: ${1:Description}",
            "  # ${99:Comment}",
            "  community.mysql.mysql_db:",
            "    ca_cert: ${2:path}",
            "    chdir: ${3:path}",
            "    check_hostname: ${4|false,true|}",
            "    check_implicit_admin: ${5|false,true|}",
            "    client_cert: ${6:path}",
            "    client_key: ${7:path}",
            "    collation: ${8:}",
            "    config_file: ${9:path}",
            "    config_overrides_defaults: ${10|false,true|}",
            "    connect_timeout: ${11:30}",
            "    dump_extra_args: ${12:string}",
            "    encoding: ${13:}",
            "    force: ${14|false,true|}",
            "    hex_blob: ${15|false,true|}",
            "    ignore_tables:",
            "      - ${16:element}",
            "    login_host: ${17:localhost}",
            "    login_password: ${18:string}",
            "    login_port: ${19:3306}",
            "    login_unix_socket: ${20:string}",
            "    login_user: ${21:string}",
            "    master_data: ${22:0}",
            "    name:",
            "      - ${23:element}",
            "    pipefail: ${24|false,true|}",
            "    quick: ${25|true,false|}",
            "    restrict_config_file: ${26|false,true|}",
            "    single_transaction: ${27|false,true|}",
            "    skip_lock_tables: ${28|false,true|}",
            "    state: ${29|present,absent,dump,import|}",
            "    target: ${30:path}",
            "    unsafe_login_password: ${31|false,true|}",
            "    use_shell: ${32|false,true|}",
            "$0"
        ],
        "description": "Add or remove MySQL databases from a remote host",
        "scope": "ansible"
    },
