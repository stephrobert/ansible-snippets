{
    "Load sops-encrypted variables from files, dynamically within a task": {
        "prefix": [
            "community.sops.load_vars",
            "Load sops-encrypted variables from files, dynamically within a task"
        ],
        "body": [
            "- name: ${1:Description}",
            "  # ${99:Comment}",
            "  community.sops.load_vars:",
            "    age_key: ${2:string}",
            "    age_keyfile: ${3:path}",
            "    aws_access_key_id: ${4:string}",
            "    aws_profile: ${5:string}",
            "    aws_secret_access_key: ${6:string}",
            "    aws_session_token: ${7:string}",
            "    config_path: ${8:path}",
            "    enable_local_keyservice: ${9|false,true|}",
            "    expressions: ${10|ignore,evaluate-on-load|}",
            "    file: ${11:path}",
            "    keyservice:",
            "      - ${12:element}",
            "    name: ${13:string}",
            "    sops_binary: ${14:path}",
            "$0"
        ],
        "description": "Load sops-encrypted variables from files, dynamically within a task",
        "scope": "ansible"
    },
    "Encrypt data with sops": {
        "prefix": [
            "community.sops.sops_encrypt",
            "Encrypt data with sops"
        ],
        "body": [
            "- name: ${1:Description}",
            "  # ${99:Comment}",
            "  community.sops.sops_encrypt:",
            "    age:",
            "      - ${2:element}",
            "    age_key: ${3:string}",
            "    age_keyfile: ${4:path}",
            "    attributes: ${5:string}",
            "    aws_access_key_id: ${6:string}",
            "    aws_profile: ${7:string}",
            "    aws_secret_access_key: ${8:string}",
            "    aws_session_token: ${9:string}",
            "    azure_kv:",
            "      - ${10:element}",
            "    config_path: ${11:path}",
            "    content_binary: ${12:string}",
            "    content_text: ${14:string}",
            "    enable_local_keyservice: ${16|false,true|}",
            "    encrypted_regex: ${17:string}",
            "    encrypted_suffix: ${18:string}",
            "    encryption_context:",
            "      - ${19:element}",
            "    force: ${20|false,true|}",
            "    gcp_kms:",
            "      - ${21:element}",
            "    group: ${22:string}",
            "    hc_vault_transit:",
            "      - ${23:element}",
            "    keyservice:",
            "      - ${24:element}",
            "    kms:",
            "      - ${25:element}",
            "    mode: ${26|\"0755,0750,0700,0644,0640,0600,\"|}",
            "    owner: ${27:string}",
            "    path: ${28:path}",
            "    pgp:",
            "      - ${29:element}",
            "    selevel: ${30:string}",
            "    serole: ${31:string}",
            "    setype: ${32:string}",
            "    seuser: ${33:string}",
            "    shamir_secret_sharing_threshold: ${34:int}",
            "    sops_binary: ${35:path}",
            "    unencrypted_regex: ${36:string}",
            "    unencrypted_suffix: ${37:string}",
            "    unsafe_writes: ${38|false,true|}",
            "$0"
        ],
        "description": "Encrypt data with sops",
        "scope": "ansible"
    }
}