{
    "Create, Delete and Modify Host Cluster on Infinibox": {
        "prefix": [
            "infinidat.infinibox.infini_cluster",
            "Create, Delete and Modify Host Cluster on Infinibox"
        ],
        "body": [
            "- name: ${1:Description}",
            "  # ${99:Comment}",
            "  infinidat.infinibox.infini_cluster:",
            "    cluster_hosts:",
            "      - ${2:element}",
            "    name: ${3:string}",
            "    password: ${4:string}",
            "    state: ${5|present,stat,absent|}",
            "    system: ${6:string}",
            "    user: ${7:string}",
            "$0"
        ],
        "description": "Create, Delete and Modify Host Cluster on Infinibox",
        "scope": "ansible"
    },
    "Create, Delete or Modify NFS Exports on Infinibox": {
        "prefix": [
            "infinidat.infinibox.infini_export",
            "Create, Delete or Modify NFS Exports on Infinibox"
        ],
        "body": [
            "- name: ${1:Description}",
            "  # ${99:Comment}",
            "  infinidat.infinibox.infini_export:",
            "    client_list:",
            "      - ${2:element}",
            "    filesystem: ${3:string}",
            "    name: ${4:string}",
            "    password: ${5:string}",
            "    state: ${6|present,stat,absent|}",
            "    system: ${7:string}",
            "    user: ${8:string}",
            "$0"
        ],
        "description": "Create, Delete or Modify NFS Exports on Infinibox",
        "scope": "ansible"
    },
    "Create, Delete or Modify NFS Client(s) for existing exports on Infinibox": {
        "prefix": [
            "infinidat.infinibox.infini_export_client",
            "Create, Delete or Modify NFS Client(s) for existing exports on Infinibox"
        ],
        "body": [
            "- name: ${1:Description}",
            "  # ${99:Comment}",
            "  infinidat.infinibox.infini_export_client:",
            "    access_mode: ${2|RW,RO|}",
            "    client: ${3:string}",
            "    export: ${4:string}",
            "    no_root_squash: ${5|false,true|}",
            "    password: ${6:string}",
            "    state: ${7|present,stat,absent|}",
            "    system: ${8:string}",
            "    user: ${9:string}",
            "$0"
        ],
        "description": "Create, Delete or Modify NFS Client(s) for existing exports on Infinibox",
        "scope": "ansible"
    },
    "Create, Delete or Modify filesystems on Infinibox": {
