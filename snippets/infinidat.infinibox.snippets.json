            "    pool: ${4:string}",
            "    size: ${5:string}",
            "    state: ${6|present,stat,absent|}",
            "    system: ${7:string}",
            "    thin_provision: ${8|true,false|}",
            "    user: ${9:string}",
            "$0"
        ],
        "description": "Create, Delete or Modify filesystems on Infinibox",
        "scope": "ansible"
    },
