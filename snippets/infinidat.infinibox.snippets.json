    "Create, Delete and Modify network spaces on Infinibox": {
        "prefix": [
            "infinidat.infinibox.infini_network_space",
            "Create, Delete and Modify network spaces on Infinibox"
        ],
        "body": [
            "- name: ${1:Description}",
            "  # ${99:Comment}",
            "  infinidat.infinibox.infini_network_space:",
            "    interfaces:",
            "      - ${2:element}",
            "    ips:",
            "      - ${3:element}",
            "    mtu: ${4:int}",
            "    netmask: ${6:int}",
            "    network: ${7:string}",
            "    password: ${8:string}",
            "    rate_limit: ${9:int}",
            "    system: ${12:string}",
            "    user: ${13:string}",
            "$0"
        ],
        "description": "Create, Delete and Modify network spaces on Infinibox",
        "scope": "ansible"
    },
    "Create, Delete and Modify Pools on Infinibox": {
        "prefix": [
            "infinidat.infinibox.infini_pool",
            "Create, Delete and Modify Pools on Infinibox"
        ],
        "body": [
            "- name: ${1:Description}",
            "  # ${99:Comment}",
            "  infinidat.infinibox.infini_pool:",
            "    compression: ${2|true,false|}",
            "    name: ${3:string}",
            "    password: ${4:string}",
            "    size: ${5:string}",
            "    ssd_cache: ${6|true,false|}",
            "    state: ${7|present,stat,absent|}",
            "    system: ${8:string}",
            "    user: ${9:string}",
            "    vsize: ${10:string}",
            "$0"
        ],
        "description": "Create, Delete and Modify Pools on Infinibox",
        "scope": "ansible"
    },
    "Add and Delete fiber channel and iSCSI ports to a host on Infinibox": {
        "prefix": [
            "infinidat.infinibox.infini_port",
            "Add and Delete fiber channel and iSCSI ports to a host on Infinibox"
        ],
        "body": [
            "- name: ${1:Description}",
            "  # ${99:Comment}",
            "  infinidat.infinibox.infini_port:",
            "    iqns:",
            "      - ${3:element}",
            "    password: ${4:string}",
            "    state: ${5|present,stat,absent|}",
            "    system: ${6:string}",
            "    user: ${7:string}",
            "    wwns:",
            "      - ${8:element}",
            "$0"
        ],
        "description": "Add and Delete fiber channel and iSCSI ports to a host on Infinibox",
        "scope": "ansible"
    },
    "Create, Delete and Modify a User on Infinibox": {
        "prefix": [
            "infinidat.infinibox.infini_user",
            "Create, Delete and Modify a User on Infinibox"
        ],
        "body": [
            "- name: ${1:Description}",
            "  # ${99:Comment}",
            "  infinidat.infinibox.infini_user:",
            "    password: ${2:string}",
            "    state: ${3|present,stat,reset_password,absent|}",
            "    system: ${4:string}",
            "    user: ${5:string}",
            "    user_email: ${6:string}",
            "    user_enabled: ${7|true,false|}",
            "    user_name: ${8:string}",
            "    user_password: ${9:string}",
            "    user_pool: ${10:string}",
            "    user_role: ${11:string}",
            "$0"
        ],
        "description": "Create, Delete and Modify a User on Infinibox",
        "scope": "ansible"
    },
    "Create, Delete or Modify volumes on Infinibox": {
        "prefix": [
            "infinidat.infinibox.infini_vol",
            "Create, Delete or Modify volumes on Infinibox"
        ],
        "body": [
            "- name: ${1:Description}",
            "  # ${99:Comment}",
            "  infinidat.infinibox.infini_vol:",
            "    password: ${4:string}",
            "    snapshot_lock_only: ${9|false,true|}",
            "    system: ${11:string}",
            "    thin_provision: ${12|true,false|}",
            "    user: ${13:string}",
            "$0"
        ],
        "description": "Create, Delete or Modify volumes on Infinibox",
        "scope": "ansible"
    }
}